<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Weidaicheng</Authors>
    <Version>1.0.0</Version>
    <Description>Extensions for jwt(https://github.com/jwt-dotnet/jwt) and asp.net core 2.0</Description>
    <Copyright>Copyright (c) 2018 Weidaicheng</Copyright>
    <PackageLicenseUrl>https://github.com/Weidaicheng/Jwt.Extensions/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Weidaicheng/Jwt.Extensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Weidaicheng/Jwt.Extensions</RepositoryUrl>
    <PackageReleaseNotes>version 1.0 includes AddJwt and the overriding of OnActionExecuting method</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JWT" Version="4.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.0.8" />
  </ItemGroup>

</Project>
